{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"baseUrl": ""
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "next.config.js", "pages/chroma-key.jsx", "components/chroma-key/chroma-key.component.jsx"],
	"exclude": ["node_modules"]
}